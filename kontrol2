#include <iostream>
using namespace std;

int yukseklikbelirle(int suseviyesi) {
	if (suseviyesi < 30)
		return 1;
	else if (suseviyesi <= 70)
		return 2;
	else
		return 3;
}


int su;
int main() {

	int suseviyesi;

	while (true) {

		cout << "Su seviyesini girin: ";
		cin >> suseviyesi;

		int mod = yukseklikbelirle(suseviyesi);

		if (70<=su<=100) {
			cout << "Normal: " << su << endl;
		}
		else if (30<=su<=70) {
			cout << "tassaruf " << su << endl;
		}
		else if (0<su<=30) {
			cout << "Acil su dolumu: " << su << endl;
		}
		else  {
			cout << "sistem durduruldu" << su << endl;
			cout << endl;
		}

		return 0;
	}
}
